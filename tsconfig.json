{
  "compilerOptions": {
    "outDir": "./lib",
    "module": "commonjs",
    "target": "es6",
    "allowJs": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "strict": true,
    "declaration": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "test"
  ]
}
